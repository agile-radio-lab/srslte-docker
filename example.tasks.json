{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "create",
            "command": "~/srslte-docker/scripts/create.sh",
            "type": "shell",
            "args": [
                "srsall_dev",
                "srsall",
                "~/srslte-docker/srsall/Dockerfile",
                "${workspaceFolder}"
            ],
            "problemMatcher": [
            ]
        },
        {
            "label": "build",
            "command": "~/srslte-docker/scripts/rebuild.sh",
            "type": "shell",
            "args": [
                "srsall_dev",
                "srsall",
                "${workspaceFolder}"
            ],
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "run eNB",
            "command": "~/srslte-docker/scripts/run.sh",
            "type": "shell",
            "args": [
                "srsall_dev",
                "srsenb",
                "enb.conf",
                "srsall"
            ],
            "problemMatcher": [
            ]
        },
        {
            "label": "run EPC",
            "command": "~/srslte-docker/scripts/run.sh",
            "type": "shell",
            "args": [
                "srsall_dev",
                "srsepc",
                "epc.conf",
                "srsall"
            ],
            "problemMatcher": [
            ]
        },
        {
            "label": "run UE",
            "command": "~/srslte-docker/scripts/run.sh",
            "type": "shell",
            "args": [
                "srsall_dev",
                "srsue",
                "ue.conf",
                "srsall"
            ],
            "problemMatcher": [
            ]
        }
    ],
    "reveal": "always"
}